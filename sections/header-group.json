{
  "name": "t:sections.header.name",
  "type": "header",
  "sections": {
    "announcement-bar": {
      "type": "announcement-bar",
      "blocks": {
        "announcement-bar-0": {
          "type": "announcement",
          "settings": {
            "text": "Welcome to your favorite store",
            "link": "{{ shop.brand.favicon_url }}"
          }
        }
      },
      "block_order": [
        "announcement-bar-0"
      ],
      "settings": {
        "color_scheme": "accent-1",
        "show_line_separator": true,
        "show_social": true,
        "auto_rotate": true,
        "change_slides_speed": 4,
        "enable_country_selector": false,
        "enable_language_selector": false
      }
    },
    "marquee-bar": {
      "type": "marquee-bar",
      "settings": {
        "text": "NO SHIPPING EXPENSS",
        "text_2": "30 Days Refund",
        "text_3": "Customer Service 24\/7",
        "colorBackground": "#000",
        "colorText": "#fff",
        "link": "shopify:\/\/collections\/all",
        "textDirection": "left"
      }
    },
    "header": {
      "type": "header",
      "settings": {
        "logo_position": "top-center",
        "menu": "main-menu",
        "menu_type_desktop": "mega",
        "sticky_header_type": "on-scroll-up",
        "show_line_separator": true,
        "color_scheme": "inverse",
        "menu_color_scheme": "accent-1",
        "enable_country_selector": true,
        "enable_language_selector": true,
        "mobile_logo_position": "center",
        "margin_bottom": 0,
        "padding_top": 8,
        "padding_bottom": 12
      }
    }
  },
  "order": [
    "announcement-bar",
    "header"
  ]
}
